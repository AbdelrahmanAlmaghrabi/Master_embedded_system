#include <stdio.h>

int main() {
	int i , x ,l, y;
	int arr[10]={0};

	for(i=0;i<10;i++){
		arr[i]=i+1;
	}
	printf("Enter number of elements : ");
	fflush(stdin);fflush(stdout);
	scanf("%d",&y);

	for(i=0;i<y;i++){
		printf("%d ",arr[i]);
	}
	printf("\n");
	printf("Enter the elements to be inserted :");
	fflush(stdin);fflush(stdout);
	scanf("%d",&x);
	printf("Enter the location :");
	fflush(stdin);fflush(stdout);
	scanf("%d",&l);
	l=l-1;

	for(i=y;i>=l;i--){
		arr[i]=arr[i-1];

	}

	y++;
	arr[l]=x;

	for(i=0;i<y;i++){
		printf("%d ",arr[i]);
	}

	return 0;
}
