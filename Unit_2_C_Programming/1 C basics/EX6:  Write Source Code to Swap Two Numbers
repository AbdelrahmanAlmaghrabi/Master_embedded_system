#include "stdio.h"

int main() {


	float a , b ,x ;
	printf("enter value of a : ");
	fflush(stdout);
	scanf("%f",&a);
	printf("enter value of b : ");
	fflush(stdout);
	scanf("%f",&b);
	x=a;
	a=b;
	b=x;

	printf("After swapping a = %0.2f\n" ,a );
	printf("After swapping b = %0.2f" ,b );
	return 0 ;

}
